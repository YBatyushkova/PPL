#lang racket

(define (hello)
 (displayln "hello world"))


(define (len L) ;; length of the list L
  (define (tail-len L k)
    (if (null? L) k
        (tail-len (cdr L) (+ k 1))))
  (tail-len L 0))

(len '(1 3 4))
(length '(1 3 4))


(define (prefix n L) ;; n first elements of the list L
  (if (= n 0)
      '()
      (cons (car L)
            (prefix (- n 1) (cdr L)))))

(prefix 3 '(1 2 5 6 7 9 1))
(take '(1 2 5 6 7 9 1) 3)